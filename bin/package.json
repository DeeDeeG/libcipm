{
  "name": "cipm",
  "repository": "https://github.com/npm/libcipm",
  "version": "2.0.1",
  "description": "a standalone ci-oriented package installer for npm projects",
  "bin": "./cli.js",
  "main": "./cli.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "docs": "tail -n +2 README.md | marked-man --manual 'User Commands' --version \"$npm_package_name@$npm_package_version\" > $npm_package_name.1"
  },
  "keywords": [
    "npm",
    "package manager",
    "caching",
    "downloader",
    "npm ci"
  ],
  "author": {
    "name": "Kat March√°n",
    "email": "kzm@sykosomatic.org",
    "twitter": "maybekatz"
  },
  "license": "ISC",
  "dependencies": {
    "libcipm": "^2.0.0",
    "npmlog": "^4.1.2",
    "yargs": "^11.0.0"
  },
  "bundleDependencies": [
    "npmlog",
    "yargs",
    "libcipm"
  ]
}
